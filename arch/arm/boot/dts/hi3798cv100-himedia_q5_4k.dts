/dts-v1/;
#include "hi3798cv100.dtsi"

/ {
	model = "HiMedia Q5 4K";
	compatible = "hisilicon,hix5hd2";

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory {
		device_type = "memory";
		reg = <0x00000000 0x80000000>;
	};
};

&timer1 {
	status = "okay";
};

&uart0 {
	status = "okay";
};

&mmc {
	status = "okay";
};

&sd {
	status = "okay";
};

&ohci {
	status = "okay";
};

&ehci {
	status = "okay";
};

&xhci {
	status = "okay";
};

&gmac0 {
	#address-cells = <1>;
	#size-cells = <0>;
	phy-handle = <&phy2>;
	phy-mode = "rgmii";
	/* Placeholder, overwritten by bootloader */
	mac-address = [00 00 00 00 00 00];
	status = "okay";

	phy2: ethernet-phy@1 {
		reg = <1>;
	};
};

&gmac1 {
	#address-cells = <1>;
	#size-cells = <0>;
	phy-handle = <&phy1>;
	phy-mode = "rgmii";
	/* Placeholder, overwritten by bootloader */
	mac-address = [00 00 00 00 00 00];
	status = "okay";

	phy1: ethernet-phy@0 {
		reg = <0>;
	};
};


