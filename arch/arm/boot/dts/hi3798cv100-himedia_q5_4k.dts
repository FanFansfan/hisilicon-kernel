/dts-v1/;
#include "hi3798cv100.dtsi"

/ {
	model = "HiMedia Q5 4K";
	compatible = "hisilicon,hix5hd2";

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory {
		device_type = "memory";
		reg = <0x00000000 0x80000000>;
	};
};

&timer1 {
	status = "okay";
};

&uart0 {
	status = "okay";
};

&mmc {
	status = "okay";
};

&sd {
	status = "okay";
};

&ohci {
	status = "okay";
};

&ehci {
	status = "okay";
};

&xhci {
	status = "okay";
};


